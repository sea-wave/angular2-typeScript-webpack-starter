{
  "name": "angular2-typescript-webpack-starter",
  "version": "1.1.0",
  "description": "Minimal Angular2 development environment which use typeScript.",
  "main": "webpack.config.js",
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "expose-loader": "0.7.1",
    "ts-loader": "0.8.0",
    "typescript": "1.7.5",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.14.1"
  },
  "scripts": {
    "prestart": "npm install",
	"bundle": "mkdir ./dist 2> /dev/null && cp src/index.html ./dist/ & webpack -p --devtool sourcemap ./dist/bundle.js",
    "start": "webpack-dev-server --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirkaufman/angular2-typeScript-webpack-starter.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "typeScript",
    "webpack",
    "angular2",
    "starter"
  ],
  "author": "Nir Kaufman <nirkaufman.com> (nirkaufman.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nirkaufman/angular2-typeScript-webpack-starter/issues"
  },
  "homepage": "https://github.com/nirkaufman/angular2-typeScript-webpack-starter#readme"
}
