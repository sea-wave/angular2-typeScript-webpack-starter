{
  "name": "angular2-typescript-webpack-starter",
  "version": "1.1.0",
  "description": "Minimal Angular2 development environment which use typeScript.",
  "main": "webpack.config.js",
  "dependencies": {
    "@angular/http": "2.0.0-rc.1",
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "core-js": "^2.3.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
	"typings" : "0.8.1",
    "expose-loader": "0.7.1",
    "ts-loader": "0.8.0",
    "typescript": "1.8.10",
    "webpack": "1.12.12",
    "webpack-dev-server": "1.14.1"
  },
  "scripts": {
	"postinstall": "typings install",
    "prestart": "npm install",
	"bundle": "mkdir ./dist 2> /dev/null & cp src/index.html ./dist/ & webpack -p --devtool sourcemap ./dist/bundle.js",
    "start": "webpack-dev-server --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirkaufman/angular2-typeScript-webpack-starter.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "typeScript",
    "webpack",
    "angular2",
    "starter"
  ],
  "author": "Nir Kaufman <nirkaufman.com> (nirkaufman.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nirkaufman/angular2-typeScript-webpack-starter/issues"
  },
  "homepage": "https://github.com/nirkaufman/angular2-typeScript-webpack-starter#readme"
}
